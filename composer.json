{
	"name": "phpstan/phpstan-sentry",
	"description": "Consistence Sentry class reflection extension for PHPStan",
	"license": ["MIT"],
	"minimum-stability": "dev",
	"prefer-stable": true,
	"require": {
		"php": "~7.0",
		"phpstan/phpstan": "*",
		"consistence/consistence": "0.22"
	},
	"require-dev": {
		"consistence/coding-standard": "~0.12.0",
		"phing/phing": "^2.13.0",
		"phpunit/phpunit": "^5.3",
		"slevomat/coding-standard": "dev-php7#d4a1a9c"
	},
	"autoload": {
		"psr-4": {
			"PHPStan\\": "src/"
		}
	},
	"autoload-dev": {
		"classmap": ["tests/"]
	}
}
